Options +ExecCGI
AddHandler cgi-script .cgi
Options +FollowSymLinks
RewriteEngine on
RewriteRule ^.*iphone\.html$ /memo.cgi?short=iphone.html [L]
RewriteRule ^dict\.js$ /memo.cgi?short=dict.js [L]
RewriteRule ^([^\.]*)$ /memo.cgi?short=$1 [L]

